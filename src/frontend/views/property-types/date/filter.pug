- filterId = property.name() + '-filter'
.picker-name= property.label()
  .date-range(id=filterId)
    .filter
      label.label='from'
        .control.has-icons-right
          input.input.filter.pickadate(name=`${filterKey}.from` value=value.from)
          span.icon.is-small.is-right
            i.icomoon-calendar
    .filter
      label.label='to'
        .control.has-icons-right
          input.input.filter.pickadate(name=`${filterKey}.to` value=value.to)
          span.icon.is-small.is-right
            i.icomoon-calendar

script.
  $(document).ready(() => {
    $('##{filterId} .pickadate').flatpickr({
      format: 'Y-m-d',
    })
  })