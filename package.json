{
  "name": "adminjs",
  "version": "6.2.0-dev.1",
  "description": "Admin panel for apps written in node.js",
  "main": "lib/index.js",
  "types": "types/index.d.ts",
  "type": "module",
  "scripts": {
    "test": "mocha ./spec/index.js",
    "clean": "rimraf lib && mkdir lib && rimraf types && mkdir types",
    "build": "tsc",
    "dev": "tsc -w",
    "lint": "eslint './spec/**/*' './src/**/*' './*'",
    "cspell": "cspell src/**/*.ts src/**/*.js src/**/*.tsx src/**/*.jsx",
    "check:all": "yarn build && yarn cspell && yarn lint && yarn test",
    "release": "semantic-release",
    "prepare": "npx husky install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SoftwareBrothers/adminjs.git"
  },
  "keywords": [
    "hapi",
    "express",
    "mongoose",
    "admin",
    "admin-panel"
  ],
  "browserslist": [
    "last 10 Chrome versions"
  ],
  "husky": {
    "hooks": {
      "commit-msg": "npx husky install && commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "author": "Wojciech Krysiak",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SoftwareBrothers/adminjs/issues"
  },
  "homepage": "https://github.com/SoftwareBrothers/adminjs#readme",
  "dependencies": {
    "flat": "^4.1.0",
    "i18next": "^21.9.1",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/flat": "^0.0.28",
    "@types/lodash": "^4.14.182",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "cspell": "^6.6.1",
    "eslint": "^8.22.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.26.0",
    "factory-girl": "^5.0.4",
    "husky": "^8.0.1",
    "require.all": "^2.0.5",
    "rimraf": "^3.0.2",
    "semantic-release": "^19.0.3",
    "semantic-release-slack-bot": "^3.5.3",
    "typescript": "^4.7.4"
  }
}
