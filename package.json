{
  "name": "adminjs",
  "version": "6.2.0-dev.2",
  "description": "Admin panel for apps written in node.js",
  "main": "lib/index.js",
  "types": "types/index.d.ts",
  "scripts": {
    "test": "mocha ./spec/index.cjs",
    "clean": "rimraf lib && mkdir lib && rimraf types && mkdir types",
    "build": "tsc",
    "dev": "tsc -w",
    "lint": "eslint './spec/**/*' './src/**/*' './*'",
    "cspell": "cspell src/**/*.ts src/**/*.js src/**/*.tsx src/**/*.jsx",
    "check:all": "yarn build && yarn cspell && yarn lint && yarn test",
    "release": "semantic-release",
    "prepare": "npx husky install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SoftwareBrothers/adminjs.git"
  },
  "keywords": [
    "hapi",
    "express",
    "mongoose",
    "admin",
    "admin-panel"
  ],
  "browserslist": [
    "last 10 Chrome versions"
  ],
  "husky": {
    "hooks": {
      "commit-msg": "npx husky install && commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "author": "Wojciech Krysiak",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SoftwareBrothers/adminjs/issues"
  },
  "homepage": "https://github.com/SoftwareBrothers/adminjs#readme",
  "peerDependencies": {
    "@adminjs/common": "^1.0.0-dev.3"
  },
  "dependencies": {
    "flat": "^4.1.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@adminjs/common": "^1.0.0-dev.3",
    "@babel/core": "^7.18.13",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/register": "^7.18.9",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/chai": "^4.1.2",
    "@types/chai-as-promised": "^7.1.2",
    "@types/cypress": "^1.1.3",
    "@types/flat": "^0.0.28",
    "@types/lodash": "^4.14.182",
    "@types/mocha": "^8.0.3",
    "@types/node": "^16",
    "@types/sinon": "^9.0.6",
    "@types/sinon-chai": "^3.2.5",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "chai-change": "^2.1.2",
    "cspell": "^6.6.1",
    "eslint": "^8.22.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-mocha": "^10.1.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "factory-girl": "^5.0.4",
    "husky": "^8.0.1",
    "mocha": "^8.1.3",
    "require.all": "^2.0.5",
    "rimraf": "^3.0.2",
    "semantic-release": "^19.0.3",
    "semantic-release-slack-bot": "^3.5.3",
    "sinon": "^9.0.3",
    "sinon-chai": "^3.5.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}
